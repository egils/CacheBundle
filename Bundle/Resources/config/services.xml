<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="egils_cache.cache_manager.class">Egils\Component\Cache\CacheManager</parameter>
        <parameter key="egils_cache.adapter.default.class">Egils\Component\Cache\Adapter\DefaultCacheAdapter</parameter>
    </parameters>

    <services>
        <service id="egils_cache.cache_manager" class="%egils_cache.cache_manager.class%">
        </service>
    </services>

</container>